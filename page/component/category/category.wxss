@import '../../common/common.wxss';

.main{
  width: 100%;
  height: 100%;
  border-top: 2rpx solid #eeeeee ;
}

.itembox{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.itemtit{
  /* flex: 1; */
  width: calc(33.33% - 5rpx);
  height: 250rpx;
  border: 1rpx solid #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.itemImg{
  width: 63rpx;
  height: 63rpx;
}
.itemText{
  margin-top: 30rpx;
  font-size: 26rpx;
  color: #191919;
  font-family: MicrosoftYaHei;
}




























